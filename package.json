  },
  "dependencies": {
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0"
  },
  "keywords": ["capacitor", "mobile", "ios"],
  "author": "Circinus",
  "license": "MIT"
}