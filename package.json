{
  "name": "contemplations-app",
  "version": "1.0.0",
  "description": "Mobile app for contemplations",
  "main": "index.html",
  "scripts": {
    "build": "mkdir -p dist && cp index.html dist/ && cp -r assets dist/ 2>/dev/null || true",
